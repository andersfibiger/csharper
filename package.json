{
  "name": "csharper",
  "displayName": "csharper mocking",
  "publisher": "AndersFibi",
  "author": {
    "name": "Anders Fibiger"
  },
  "description": "Helps mocking dependencies for a class",
  "version": "0.1.1",
  "repository": {
    "url": "https://github.com/andersfibiger/csharper"
  },
  "icon": "csharp-icon.png",
  "engines": {
    "vscode": "^1.53.0"
  },
  "keywords": [
    "c# test",
    "dotnet test",
    "dotnet mocking",
    "c# mocking",
    "c# mock tool"
  ],
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:csharp"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [],
    "languages": [
      {
        "id": "csharp",
        "aliases": [
          "csharp",
          "c#"
        ],
        "extensions": [
          ".cs"
        ],
        "filenamePatterns": [
          "*Test",
          "*Tests"
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.53.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  }
}
